	}
	else
	{
		displayString += 		"<progress id='"+statStyle+"' @value=\"$CharacterSheet_Player.fight.variables."+currentStatVariable+"\" @max=\"" + statValueString + "\"></progress>";
	}
	
	displayString += 	"</div>";
