setup.getLength = function(value)
{
	var displayString = "";
	var shownUnits = 2;

	if(State.variables.Measurement == setup.Units.METRIC)
	{
		var cm = Math.floor(value * 2.54);
		if(cm / 100000 >= 1 && shownUnits != 0)
		{
			var km = Math.floor(cm / 100000);
			cm -= km * 100000;
			
			if(km > 1000000000)
			{
				displayString += (km / 1000000000).toFxied(0).replace(/\.00$/, '') + "百万千米&nbsp;";
				shownUnits = 0
			}
			else if(km > 1000000)
			{
				displayString += (km / 1000000).toFixed(0).replace(/\.00$/, '') + "千千米&nbsp;";
				shownUnits = 0;
			}
			else
			{
				displayString += km.toFixed(2).replace(/\.00$/, '') + "千米&nbsp;" ;
				shownUnits--;
			}
		}

		if(cm / 100 >= 1 && shownUnits != 0)
		{
            if(cm > 0)
            {
                var m = Math.floor(cm / 100);
                cm -= m * 100;
    
                displayString += m.toFixed(2).replace(/\.00$/, '') + "米&nbsp;" ;
            }

			shownUnits--;
		}

		if(cm < 100 && shownUnits != 0)
		{
            if(cm > 0)
            {
                displayString += cm.toFixed(2).replace(/\.00$/, '') % 100+"厘米";
            }

			shownUnits--;
		}
	}
	else
	{
		if(value / 63360 >= 1 && shownUnits != 0)
		{
			var miles = Math.floor(value / 63360);
			value -= miles * 63360;

			if(miles > 1000000000)
			{
				displayString += (miles / 1000000000).toFixed(0).replace(/\.00$/, '') + "百万英里&nbsp;";
				shownUnits = 0
			}
			else if(miles > 1000000)
			{
				displayString += (miles / 1000000).toFixed(0).replace(/\.00$/, '') + "千英里&nbsp;";
				shownUnits = 0;
			}
			else
			{
				displayString += miles.toFixed(2).replace(/\.00$/, '') + "英里&nbsp;" ;
				shownUnits--;
			}
		}

		if(value / 12 >= 1 && shownUnits != 0)
		{
            if(value > 0)
            {
                var ft = Math.floor(value / 12);
                value -= ft * 12;
    
                displayString += ft.toFixed(2).replace(/\.00$/, '') + "英尺&nbsp;";
            }

			shownUnits--;
		}

		if(value < 12 && shownUnits != 0)
		{
            if(value > 0)
            {
                displayString += (value % 12).toFixed(2).replace(/\.00$/, '') +"英寸";
            }
            
			shownUnits--;
		}
	}

	return displayString;
};

