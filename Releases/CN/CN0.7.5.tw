/*
    notify.min.css, by chapel; for use with notify.js
    version 1.0.0
 */
#notify {
    position : fixed;
    display : block;
    width : 26em;
    right : -30em; top : 2em;
    padding : 0.5em;
    -webkit-transition : right 0.3s;
    -moz-transition : right 0.3s;
    -o-transition : right 0.3s;
    transition : right 0.3s;
    text-align: center;
    z-index: 9999;
}
#notify.open { right : 0; }

#notify.Corrupt0
{
  border: 21px solid;
