    var _vowels = /[aeiou8]/i;
    var _acronyms = /[A-Z]+$/;
    var _irregularAcronyms = /[UFHLMNRSX]/;
    var _punctuation = /[.,\/#!$%\^&\*;:{}=\-_`~()]/g;

