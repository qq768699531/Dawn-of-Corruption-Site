    "update",
    setup.AbilityTrigger.ON_DAY_CHANGE,
    function(abilityRuntimeData, abilityParams)
    {  
        if(abilityRuntimeData.isActive == true)
        {
            if(abilityParams.self.info.accumulatedCorruption <= 0)
            {
                abilityRuntimeData.isActive = false;
            }
        }
    }
)>>

